// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Invoices {
  id              String @id @default(uuid())
  numClient       String
  mesRef          String
  energiaEletrica Int
  energiaICMS     Int
  energiaComp     Int
  contIlumPub     Int

  @@map("invoices")
}
